<?xml version="1.0" encoding="utf-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>rguestpayshim</artifactId>
    <packaging>pom</packaging>
    <version>74.1888.0</version>
    <name>rGuestPay Shim Parent</name>
    <description>Parent pom for rGuestPay Shim</description>
    <parent>
        <groupId>com.agilysys.pms</groupId>
        <artifactId>stay-root-pom</artifactId>
        <version>74.1861.0</version>
    </parent>
    <properties>
        <maven.jar.plugin.version>2.4</maven.jar.plugin.version>
        <sonar.exclusions>**/domain/*.java,**/model/*.java,**/*Exception.java,**/reference/*.java,**/*Constants.java,**/*.spec.js,**/com/freedompay/**/*.java</sonar.exclusions>
    </properties>
    <modules>
        <module>ServiceInterface</module>
    </modules>
    <!-- All the third party dependencies should be in VictorsRootPom. -->
    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.agilysys.pms</groupId>
                <artifactId>rguestpayshim-interface</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.pms</groupId>
                <artifactId>common-interface</artifactId>
                <version>74.1861.0</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.pms</groupId>
                <artifactId>common-interface</artifactId>
                <version>74.1861.0</version>
                <type>test-jar</type>
                <scope>test</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>
    <dependencies>
        <dependency>
            <groupId>com.agilysys.platform</groupId>
            <artifactId>common</artifactId>
        </dependency>
        <dependency>
            <groupId>com.agilysys.platform</groupId>
            <artifactId>common</artifactId>
            <scope>test</scope>
            <type>test-jar</type>
        </dependency>
    </dependencies>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-jar-plugin</artifactId>
                    <version>${maven.jar.plugin.version}</version>
                </plugin>
                <plugin>
                    <groupId>org.jacoco</groupId>
                    <artifactId>jacoco-maven-plugin</artifactId>
                    <configuration>
                        <excludes>
                            <exclude>com/agilysys/**/domain/*.class</exclude>
                            <exclude>com/agilysys/**/model/*.class</exclude>
                            <exclude>com/agilysys/**/config/*.class</exclude>
                        </excludes>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
    <scm>
        <connection>scm:git:ssh://git@stash.agilysys.local:7999/pms/rguestpayshim.git</connection>
        <developerConnection>scm:git:ssh://git@stash.agilysys.local:7999/pms/rguestpayshim.git</developerConnection>
        <tag>rguestpayshim-74.1888.0</tag>
    </scm>
</project>