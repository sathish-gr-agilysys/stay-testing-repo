    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <executions>
                    <execution>
                        <goals>
                            <goal>test-jar</goal>
                        </goals>
                        <configuration>
                            <includes>
                                <include>**/*.properties</include>
                                <include>com/agilysys/common/model/testproviders/*.class</include>
                                <include>com/agilysys/common/test/*.class</include>
                                <include>com/agilysys/pms/common/auth/config/StayApiKeyAuthITConfig.class</include>
                                <include>com/agilysys/pms/common/data/TargetedJobRepositoryIT.class</include>
                                <include>com/agilysys/pms/common/data/config/*.class</include>
                                <include>com/agilysys/pms/common/data/model/*.class</include>
                                <include>com/agilysys/pms/common/data/support/MongoSeeder.class</include>
                                <include>com/agilysys/pms/common/locks/ScopeLockMockUtils.class</include>
                                <include>com/agilysys/pms/common/managedop/support/ParticipantIdempotencyTest.class</include>
                                <include>com/agilysys/pms/common/messaging/FanoutCacheTest.class</include>
                                <include>com/agilysys/pms/common/messaging/core/MessagingAwareTest.class</include>
                                <include>com/agilysys/pms/common/recovery/*.class</include>
                                <include>com/agilysys/pms/common/recovery/tasks/*.class</include>
                                <include>com/agilysys/pms/common/recovery/tasks/config/*.class</include>
                                <include>com/agilysys/pms/common/recovery/tasks/alpha/*.class</include>
                                <include>com/agilysys/pms/common/recovery/support/ConflictResolverTestHelper.class</include>
                                <include>com/agilysys/pms/common/secondarywrites/SecondaryWritesWatchdogTaskIT.class</include>
                                <include>com/agilysys/pms/common/secondarywrites/support/SecondaryWritesTestHelper.class</include>
                                <include>com/agilysys/pms/common/secondarywrites/support/CompoundId.class</include>
                                <include>com/agilysys/pms/common/support/*.class</include>
                                <include>com/agilysys/pms/common/support/config/*.class</include>
                                <include>com/agilysys/pms/common/testpool/*.class</include>
                                <include>com/agilysys/pms/persistence/mongo/*.class</include>
                                <include>com/agilysys/pms/gdpr/GdprDataProvider.class</include>
                                <include>com/agilysys/pms/tasks/*.class</include>
                                <include>mongo/**</include>
                                <include>startup/**</include>
                            </includes>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>generate-test-bootstrap-data</id>
                        <phase>process-test-classes</phase>
                        <goals>
                            <goal>java</goal>
                        </goals>
                        <configuration>
                            <classpathScope>test</classpathScope>
                            <mainClass>com.agilysys.pms.common.support.PlatformBootstrapForIT</mainClass>
                            <arguments>
                                <argument>${project.build.directory}/test-bootstrap</argument>
                            </arguments>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-assembly-plugin</artifactId>
                <executions>
                    <execution>
                        <id>package-test-bootstrap-data</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <descriptors>
                                <descriptor>src/test/assembly/test-bootstrap-data.xml</descriptor>
                            </descriptors>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
            </plugin>
        </plugins>
        <resources>
            <!-- Default resource directory. -->
            <resource>
                <directory>src/main/resources</directory>
            </resource>
            <!-- Adds .orm &amp; .jdo resources to project. -->
            <resource>
                <directory>src/main/java</directory>
                <includes>
                    <include>**/*.orm</include>
                    <include>**/*.jdo</include>
                </includes>
            </resource>
        </resources>
    </build>
