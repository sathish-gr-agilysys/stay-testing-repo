<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <groupId>com.agilysys.platform</groupId>
    <artifactId>platform-dependency-management</artifactId>
    <version>5.0.48</version>
    <packaging>pom</packaging>

    <name>Platform Dependency Management</name>
    <description>
        Dependency Management to include when consuming Platform Services and Libraries
    </description>

    <modules>
        <module>dependency-convergence-test</module>
    </modules>

    <properties>
        <authservice.version>5.0.21</authservice.version>
        <communicationservice.version>5.0.23</communicationservice.version>
        <contentservice.version>5.0.33</contentservice.version>
        <deviceservice.version>5.0.42</deviceservice.version>
        <fleetservice.version>5.0.54</fleetservice.version>
        <gatewayservice.version>5.0.26</gatewayservice.version>
        <loyaltyservice.version>5.0.22</loyaltyservice.version>
        <platform-internal-libraries.version>5.0.10</platform-internal-libraries.version>
        <platform-libraries.version>5.0.18</platform-libraries.version>
        <platformauth.version>5.0.6</platformauth.version>
        <servicestubs.version>1.20.5</servicestubs.version>
        <taxservice.version>5.0.8</taxservice.version>
        <userservice.version>5.0.69</userservice.version>
        <websocketui.version>5.0.13</websocketui.version>
        <python-startup-user-creator.version>3.20.0</python-startup-user-creator.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <!-- libs -->
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>platform-internal-libraries</artifactId>
                <version>${platform-internal-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>common</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>config</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>key-management</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>auth</artifactId>
                <version>${platformauth.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>auth-ui</artifactId>
                <version>${platformauth.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>jwt-interface</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>jwt-jose4j</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <!-- toolkits -->
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>rest-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
                <exclusions>
                    <exclusion>
                        <groupId>com.wordnik</groupId>
                        <artifactId>swagger-annotations_2.10</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>crypto-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>messaging-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>monitoring-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>persistence-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>rule-engine-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
            </dependency>

            <!-- client jars -->
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>auth-interface</artifactId>
                <version>${authservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>communication-service-interface</artifactId>
                <version>${communicationservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>content-service-interface</artifactId>
                <version>${contentservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>device-interface</artifactId>
                <version>${deviceservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>fleet-interface</artifactId>
                <version>${fleetservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>gateway-service-interface</artifactId>
                <version>${gatewayservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>loyalty-interface</artifactId>
                <version>${loyaltyservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>tax-service-interface</artifactId>
                <version>${taxservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>user-interface</artifactId>
                <version>${userservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>user-service-client-lib</artifactId>
                <version>${userservice.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>websocket-ui-interface</artifactId>
                <version>${websocketui.version}</version>
            </dependency>
            <!-- test jars -->
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>auth</artifactId>
                <version>${platformauth.version}</version>
                <scope>test</scope>
                <type>test-jar</type>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>common</artifactId>
                <version>${platform-libraries.version}</version>
                <type>test-jar</type>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>messaging-toolkit</artifactId>
                <version>${platform-libraries.version}</version>
                <type>test-jar</type>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>web-socket-stub-client</artifactId>
                <version>${servicestubs.version}</version>
                <scope>test</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <scm>
        <connection>scm:git:ssh://git@stash.agilysys.local:7999/plat/platform-dependency-management.git</connection>
        <developerConnection>scm:git:ssh://git@stash.agilysys.local:7999/plat/platform-dependency-management.git
        </developerConnection>
        <tag>platform-dependency-management-5.0.48</tag>
    </scm>
</project>
