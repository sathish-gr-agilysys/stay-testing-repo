<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <artifactId>authparent</artifactId>
    <version>5.0.28.7</version>
    <packaging>pom</packaging>
    <name>Auth Parent</name>
    <description>Parent pom for Platform Auth</description>

    <parent>
        <groupId>com.agilysys.platform</groupId>
        <artifactId>parent</artifactId>
        <version>5.0.15</version>
    </parent>

    <properties>
        <platform-dependency-management.version>5.0.177.10</platform-dependency-management.version>
        <wiremock.version>2.22.0</wiremock.version>
        <project.scm.id>my-scm-server</project.scm.id>
    </properties>

    <modules>
        <module>auth</module>
        <module>authui</module>
    </modules>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>platform-dependency-management</artifactId>
                <version>${platform-dependency-management.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>com.github.ben-manes.caffeine</groupId>
                <artifactId>caffeine</artifactId>
                <version>2.7.0</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <profiles>
        <profile>
            <id>replacer</id>
            <build>
                <plugins>
                    <plugin>
                        <groupId>com.google.code.maven-replacer-plugin</groupId>
                        <artifactId>replacer</artifactId>
                        <version>1.5.3</version>
                        <configuration>
                            <basedir>.</basedir>
                        </configuration>
                        <executions>
                            <execution>
                                <id>parentpom</id>
                                <phase>process-resources</phase>
                                <goals>
                                    <goal>replace</goal>
                                </goals>
                                <configuration>
                                    <includes>
                                        <include>pom.xml</include>
                                    </includes>
                                    <regex>true</regex>
                                    <replacements>
                                        <replacement>
                                            <xpath>/project/parent/version/text()</xpath>
                                            <token>(.+)</token>
                                            <value>${replacer.parentpom.version}</value>
                                        </replacement>
                                    </replacements>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

    <scm>
        <connection>scm:git:https://dev.azure.com/agilysys/Core/_git/PlatformAuth</connection>
        <developerConnection>scm:git:https://dev.azure.com/agilysys/Core/_git/PlatformAuth</developerConnection>
        <tag>authparent-5.0.28.7</tag>
    </scm>
</project>
