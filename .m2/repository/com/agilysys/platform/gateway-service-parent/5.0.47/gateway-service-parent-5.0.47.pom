<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <artifactId>gateway-service-parent</artifactId>
    <version>5.0.47</version>
    <packaging>pom</packaging>
    <name>Gateway Parent</name>
    <description>Parent pom for Gateway Service</description>

    <parent>
        <groupId>com.agilysys.platform</groupId>
        <artifactId>service-parent</artifactId>
        <version>5.0.3</version>
    </parent>

    <modules>
        <module>interface</module>
        <module>service</module>
        <module>appliancecommon</module>
        <module>itest</module>
    </modules>

    <properties>
        <userservice.version>5.0.12</userservice.version>
        <authservice.version>5.0.3</authservice.version>
        <deviceservice.version>5.0.3</deviceservice.version>
        <fleetservice.version>5.0.1</fleetservice.version>
        <python-startup-user-creator.version>3.20.11</python-startup-user-creator.version>
        <platform-dependency-management.version>5.0.49</platform-dependency-management.version>
        <platform-libraries.version>5.0.16</platform-libraries.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>platform-dependency-management</artifactId>
                <version>${platform-dependency-management.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>

            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>gateway-service-interface</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.agilysys.platform</groupId>
                <artifactId>appliancecommon</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>com.suse.salt</groupId>
                <artifactId>salt-netapi-client</artifactId>
                <version>0.9.0</version>
            </dependency>

            <dependency>
                <!-- TODO: Get rid of this retroversion; upgrade to mockito-2. This requires some maintenence. -->
                <groupId>org.mockito</groupId>
                <artifactId>mockito-core</artifactId>
                <version>1.10.19</version>
            </dependency>

        </dependencies>
    </dependencyManagement>

    <scm>
        <connection>scm:git:ssh://git@stash.agilysys.local:7999/plat/onpremisesgateway.git</connection>
        <developerConnection>scm:git:ssh://git@stash.agilysys.local:7999/plat/onpremisesgateway.git</developerConnection>
        <tag>gateway-service-parent-5.0.47</tag>
    </scm>
</project>
